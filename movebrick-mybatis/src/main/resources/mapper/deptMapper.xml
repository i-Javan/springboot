<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper
	namespace="com.movebrick.mybatis.module.mapper.DeptMapper">
	<resultMap id="DeptMap"
		type="com.movebrick.mybatis.module.bean.Dept">
		<result column="id" property="id" />
		<result column="dname" property="dname" />
		<!-- <association property="" fetchType="lazy"></association> -->
		<collection property=""></collection>
	</resultMap>

	<sql id="Base_Column_List">
		id,dname
	</sql>

	<select id="get" resultMap="DeptMap">
		select
		<include refid="Base_Column_List" />
		from dept where id = #{id}
	</select>

</mapper>
